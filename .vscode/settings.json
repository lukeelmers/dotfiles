{
    "workbench.colorTheme": "VS Snazzy Theme",
    // "workbench.colorTheme": "White Night",
    // "workbench.colorTheme": "Oceanic Next",
    "editor.fontFamily": "'Fira Code'",
    "editor.fontSize": 13,
    "editor.fontLigatures": true,
    "workbench.iconTheme": "eq-material-theme-icons-ocean",
    // "workbench.colorCustomizations":{
    //     "tab.activeBackground": "#343D46",
    //     "activityBar.background": "#343D46",
    //     "editorGroup.background": "#1B2B34",
    //     "sideBar.background": "#1B2B34",
    //     "statusBarItem.activeBackground": "#ff0000"
    // },
    "vim.easymotion": true,
    "vim.incsearch": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "vim.insertModeKeyBindings": [
        {
            "before": ["j","j"],
            "after": ["<Esc>"]
        }
    ],
    "vim.normalModeKeyBindings": [
        {
            "before": [";"],
            "after": [":"]
        },
        {
            "before":["<leader>", "<space>"],
            "after":[],
            "commands": [
                {
                    "command": ":nohl"
                }
            ]
        }
    ],
    "vim.leader": "<space>",
    "terminal.integrated.fontFamily": "Sauce Code Pro Plus Nerd File Types",
    "gitlens.advanced.messages": {
        "suppressShowKeyBindingsNotice": true
    },
    "gitlens.historyExplorer.enabled": true,
    "window.zoomLevel": 1,
    "yaml.format.enable": true,
    "[yaml]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 4,
        "editor.quickSuggestions": {
            "other": true,
            "comments": false,
            "strings": true
        },
        "editor.autoIndent": false
    },
    "gitlens.codeLens.authors.enabled": false,
    "gitlens.codeLens.recentChange.enabled": false,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
}